{
  "hosting": {
    "public": "client/dist/client",
    "ignore": [
      "firebase.json",
      "package*",
      "client/src",
      ".*",
      "**/.*",
      "**/**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
